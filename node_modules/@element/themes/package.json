{
  "name": "@element/themes",
  "version": "5.30.0",
  "description": "",
  "main": "index.js",
  "files": [
    "lib/*",
    "switcher/*",
    "installer/*",
    "scripts/pathResolver.js",
    "scripts/postInstall.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.platforms.engineering/element/element-themes-v5.git"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:element-scss-all && npm run build:themes-installer && npm run build:storybook-themes && npm run build:dev && npm run build:min && npm run build:imports",
    "build:bayer": "npm run build:watch --theme=bayer",
    "build:clean": "npm run build:clean-lib && npm run build:clean-index",
    "build:clean-index": "rm -rf index",
    "build:clean-lib": "rm -rf lib",
    "build:component-scss-all": "node ./scripts/buildComponentScss --theme=all",
    "build:dev": "parallel-webpack -- --max-retries=3 --mode=development",
    "build:element-scss": "node ./scripts/buildElementScss",
    "build:element-scss-all": "node ./scripts/buildElementScss --theme=all",
    "build:hcp": "npm run build:watch --theme=hcp",
    "build:imports": "node ./scripts/buildImports",
    "build:min": "parallel-webpack -- --max-retries=3 --mode=production",
    "build:reset": "rm -rf node_modules && rm package-lock.json && npm i",
    "build:storybook": "npm run build:clean && npm run build:element-scss-all && npm run build:themes-installer && npm run build:storybook-themes-dev && npm run build:imports",
    "build:storybook-themes": "node ./scripts/copyAssetsToLib && node ./scripts/buildStorybookThemes && node ./scripts/buildStorybookThemesSwitcher",
    "build:storybook-themes-dev": "node ./scripts/copyAssetsToLib && node ./scripts/buildStorybookThemes --mode='dev' && node ./scripts/buildStorybookThemesSwitcher",
    "build:themes-index": "node ./scripts/buildThemeIndex",
    "build:themes-installer": "node ./scripts/buildThemeInstallerLists",
    "build:watch": "npm run build:clean && npm run build:element-scss -- --theme=all  && npm run build:themes-installer && npm run build:storybook-themes-dev && webpack --config=webpack.watch.config.js --watch --mode=development --env=theme=$npm_config_theme",
    "build:watch-all": "npm run build:clean && npm run build:element-scss-all && npm run build:themes-installer  && npm run build:storybook-themes-dev && parallel-webpack --config=webpack.watch-all.config.js --watch --mode=development",
    "build:web": "build-storybook -s ./public",
    "changelog": "npx standard-version --skip.bump --skip.commit",
    "changelog:major": "standard-version --release-as-major",
    "changelog:minor": "standard-version --release-as-minor",
    "changelog:patch": "standard-version --release-as-patch",
    "eslint:check": "eslint --ext .js --ignore-path .eslintignore .",
    "eslint:fix": "eslint --ext .js --ignore-path .eslintignore . --fix",
    "lint": "npm run stylelint:check && npm run eslint:check && npm run prettier:check",
    "postinstall": "node ./scripts/postInstall.js --runPostInstall=$npm_config_runPostInstall",
    "prepare": "husky install",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "release": "./node_modules/@element/shared/scripts/release.sh --create-branch --cerebro --cleanup --create-version --checkout-source --create-prs --exit-to-source-branch",
    "release:beta:next": "npm run release -- -v prepatch --preid beta",
    "release:beta:start": "npm run release -- -v prepatch --preid beta",
    "release:major": "npm run release -- -v major",
    "release:minor": "npm run release -- -v minor",
    "release:patch": "npm run release -- -v patch",
    "release:premajor": "npm run release -- -v premajor",
    "release:preminor": "npm run release -- -v preminor",
    "release:prepatch": "npm run release -- -v prepatch",
    "release:prerelease": "npm run release -- -v prerelease",
    "release:test": "./node_modules/@element/shared/scripts/release.sh",
    "start": "rm -rf ./node_modules/.cache && npm run build:storybook-themes-dev && npm run storybook",
    "storybook": "start-storybook -p 8008 -s ./public",
    "stylelint:check": "stylelint **/*.scss --syntax scss",
    "stylelint:fix": "stylelint **/*.scss --syntax scss --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Element Team <element-ds@bayer.com>",
  "license": "ISC",
  "dependencies": {
    "sass-hex-rgb": "^1.0.1",
    "warning": "^4.0.3",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@element/cerebro": "^3.37.0",
    "@element/eslint-config": "^0.2.0",
    "@element/prettier-config": "^1.0.0",
    "@element/shared": "^1.6.0",
    "@element/stylelint-config": "^0.5.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-google-analytics": "^6.2.9",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/client-api": "^6.4.22",
    "@storybook/html": "^6.4.22",
    "babel-loader": "^8.1.0",
    "change-case": "^4.1.2",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^4.3.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "focus-trap": "^6.1.4",
    "fs-extra": "^9.0.1",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "husky": "^7.0.0",
    "material-components-web": "^11.0.0",
    "mini-css-extract-plugin": "^1.0.0",
    "parallel-webpack": "^2.6.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "react-is": "^16.13.1",
    "sass": "1.39.2",
    "sass-loader": "^9.0.3",
    "sass-resources-loader": "^2.1.1",
    "style-loader": "^2.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.1.0"
  }
}
